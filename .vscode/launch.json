            "name": "Spring Boot-SportifyEventV1Application<SportifyEventV1>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.sportify.SportifyEventV1Application",
            "projectName": "SportifyEventV1",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=53241 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=SportifyEventV1"
